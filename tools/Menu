#!/data/data/com.termux/files/usr/bin/bash
# ğŸ”’ ç»ˆæä¿æŠ¤ç³»ç»Ÿ - ä¸PythonåŒç­‰å¼ºåº¦

set -e

ywonkkrndc=""

fioecrxfgkowigl() {
    echo "ğŸ”’ ç³»ç»ŸéªŒè¯ä¸­..."
    if [ -n "${DEBUG:-}" ]; then
        echo "ğŸš¨ å®‰å…¨è­¦å‘Š: è°ƒè¯•æ¨¡å¼æ£€æµ‹"
        exit 1
    fi
}

ilwsavndvqzr() {
    local mpeuifthhf=$(mktemp)
    cat > "${mpeuifthhf}" << 'PYTHON_SCRIPT'
import base64
import zlib
import hashlib
import sys
import tempfile
import os
from Crypto.Cipher import AES
from Crypto.Util.Padding import unpad

def main_decrypt():
    try:
        # å¯†é’¥æ´¾ç”Ÿ - ä¸åŠ å¯†æ—¶ç›¸åŒ
        key = hashlib.pbkdf2_hmac('sha256', 
                                b'ULTIMATE_BASH_ENCRYPTION_KEY_2024_XD', 
                                b'ultimate_bash_encrypt_2024', 
                                100000, 32)
        
        # è¯»å–åŠ å¯†æ•°æ®
        with open(sys.argv[1], 'r', encoding='utf-8') as f:
            encrypted_data = f.read().strip()
        
        # Base64è§£ç 
        encrypted_bytes = base64.b64decode(encrypted_data)
        
        # æå–IVå’Œå¯†æ–‡
        iv = encrypted_bytes[:16]
        ciphertext = encrypted_bytes[16:]
        
        # AESè§£å¯†
        cipher = AES.new(key, AES.MODE_CBC, iv)
        compressed_data = unpad(cipher.decrypt(ciphertext), AES.block_size)
        
        # è§£å‹ç¼©
        original_script = zlib.decompress(compressed_data)
        
        # å†™å…¥ä¸´æ—¶æ–‡ä»¶
        with tempfile.NamedTemporaryFile(mode='w', suffix='.sh', delete=False) as f:
            f.write(original_script.decode('utf-8'))
            temp_path = f.name
        
        # è®¾ç½®æƒé™å¹¶æ‰§è¡Œ
        os.chmod(temp_path, 0o755)
        os.execv(temp_path, [temp_path] + sys.argv[2:])
        
    except Exception as e:
        print("ç³»ç»Ÿå¯åŠ¨å¤±è´¥: å®‰å…¨éªŒè¯é”™è¯¯")
        return False

if __name__ == "__main__":
    main_decrypt()
PYTHON_SCRIPT

    # æ‰§è¡Œè§£å¯†
    echo "${ywonkkrndc}" > "${mpeuifthhf}_data"
    python3 "${mpeuifthhf}" "${mpeuifthhf}_data" "$@"
    
    # æ¸…ç†ï¼ˆé€šå¸¸ä¸ä¼šæ‰§è¡Œåˆ°è¿™é‡Œï¼Œå› ä¸ºexecvä¼šæ›¿æ¢è¿›ç¨‹ï¼‰
    rm -f "${mpeuifthhf}" "${mpeuifthhf}_data"
}

aquxhpqnuasxnf() {
    echo "========================================"
    echo "ğŸ”’ ç»ˆæä¿æŠ¤ç³»ç»Ÿ v2.0"
    echo "ğŸ›¡ï¸  å•†ä¸šçº§åŠ å¯†å¼ºåº¦"
    echo "========================================"
}

lrrzqkgodtglw() {
    trap 'rm -f /tmp/*_$$.* 2>/dev/null' EXIT
}

# ä¸»æ‰§è¡Œæµç¨‹
main() {
    lrrzqkgodtglw
    fioecrxfgkowigl
    aquxhpqnuasxnf
    
    echo "ğŸ”“ è§£å¯†æ‰§è¡Œä¸­..."
    ilwsavndvqzr "$@"
}

# å¯åŠ¨ç³»ç»Ÿ
main "$@"
